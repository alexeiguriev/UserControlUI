<form method="post" enctype="multipart/form-data" asp-controller="Document" asp-action="Create">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload File</p>
            <input type="file" name="file"/>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Upload file"/>
    </div>
    <div class="form-group">
        <span>@TempData["msg"]</span>
    </div>
</form>
